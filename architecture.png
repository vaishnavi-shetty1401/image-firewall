            </span><span><span class="hljs-keyword">return</span></span><span> {</span><span><span class="hljs-string">"statusCode"</span></span><span>: </span><span><span class="hljs-number">200</span></span><span>}

        </span><span><span class="hljs-comment"># ---- REKOGNITION SCAN ----</span></span><span>
        image_bytes = s3.get_object(Bucket=bucket, Key=key)[</span><span><span class="hljs-string">"Body"</span></span><span>].read()

        response = rekognition.detect_moderation_labels(
            Image={</span><span><span class="hljs-string">"Bytes"</span></span><span>: image_bytes},
