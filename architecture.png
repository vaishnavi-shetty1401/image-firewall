<pre class="overflow-visible! px-0!" data-start="403" data-end="4102"><div class="contain-inline-size rounded-2xl corner-superellipse/1.1 relative bg-token-sidebar-surface-primary"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between h-9 bg-token-sidebar-surface-primary select-none rounded-t-2xl corner-t-superellipse/1.1">python</div><div class="sticky top-[calc(--spacing(9)+var(--header-height))] @w-xl/main:top-9"><div class="absolute end-0 bottom-0 flex h-9 items-center pe-2"><div class="bg-token-bg-elevated-secondary text-token-text-secondary flex items-center gap-4 rounded-sm px-2 font-sans text-xs"><button class="flex gap-1 items-center select-none py-1" aria-label="Copy"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" aria-hidden="true" class="icon-sm"><use href="/cdn/assets/sprites-core-k5zux585.svg#ce3544" fill="currentColor"></use></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-python"><span><span><span class="hljs-keyword">import</span></span><span> boto3
</span><span><span class="hljs-keyword">import</span></span><span> uuid
</span><span><span class="hljs-keyword">from</span></span><span> datetime </span><span><span class="hljs-keyword">import</span></span><span> datetime
</span><span><span class="hljs-keyword">from</span></span><span> urllib.parse </span><span><span class="hljs-keyword">import</span></span><span> unquote_plus

</span><span><span class="hljs-comment"># AWS clients</span></span><span>
s3 = boto3.client(</span><span><span class="hljs-string">'s3'</span></span><span>)
rekognition = boto3.client(</span><span><span class="hljs-string">'rekognition'</span></span><span>)
dynamodb = boto3.resource(</span><span><span class="hljs-string">'dynamodb'</span></span><span>)

</span><span><span class="hljs-comment"># DynamoDB table</span></span><span>
AUDIT_TABLE = </span><span><span class="hljs-string">"image-firewall-audit"</span></span><span>
table = dynamodb.Table(AUDIT_TABLE)

</span><span><span class="hljs-comment"># Buckets</span></span><span>
UPLOAD_BUCKET = </span><span><span class="hljs-string">"image-firewall-upload"</span></span><span>
ALLOWED_BUCKET = </span><span><span class="hljs-string">"image-firewall-allowed"</span></span><span>
QUARANTINE_BUCKET = </span><span><span class="hljs-string">"image-firewall-quarantine"</span></span><span>

</span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
</span><span><span class="hljs-comment"># STEP 4: Audit logging function</span></span><span>
</span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">log_audit</span></span><span>(</span><span><span class="hljs-params">file_name, bucket, decision, reason, confidence</span></span><span>):
    table.put_item(
        Item={
            </span><span><span class="hljs-string">"imageId"</span></span><span>: </span><span><span class="hljs-built_in">str</span></span><span>(uuid.uuid4()),
            </span><span><span class="hljs-string">"fileName"</span></span><span>: file_name,
            </span><span><span class="hljs-string">"bucket"</span></span><span>: bucket,
            </span><span><span class="hljs-string">"decision"</span></span><span>: decision,
            </span><span><span class="hljs-string">"reason"</span></span><span>: reason,
            </span><span><span class="hljs-string">"confidence"</span></span><span>: </span><span><span class="hljs-built_in">str</span></span><span>(confidence),
            </span><span><span class="hljs-string">"timestamp"</span></span><span>: datetime.utcnow().isoformat()
        }
    )

</span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
</span><span><span class="hljs-comment"># Lambda handler</span></span><span>
</span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">lambda_handler</span></span><span>(</span><span><span class="hljs-params">event, context</span></span><span>):

    record = event[</span><span><span class="hljs-string">"Records"</span></span><span>][</span><span><span class="hljs-number">0</span></span><span>]
    bucket = record[</span><span><span class="hljs-string">"s3"</span></span><span>][</span><span><span class="hljs-string">"bucket"</span></span><span>][</span><span><span class="hljs-string">"name"</span></span><span>]
    key = unquote_plus(record[</span><span><span class="hljs-string">"s3"</span></span><span>][</span><span><span class="hljs-string">"object"</span></span><span>][</span><span><span class="hljs-string">"key"</span></span><span>])

    </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"Processing image: <span class="hljs-subst">{key}</span></span></span><span>")

    </span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
    </span><span><span class="hljs-comment"># STEP 5A: Size validation (Rekognition limit = 5MB)</span></span><span>
    </span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
    head = s3.head_object(Bucket=bucket, Key=key)
    size = head[</span><span><span class="hljs-string">"ContentLength"</span></span><span>]

    </span><span><span class="hljs-keyword">if</span></span><span> size &gt; </span><span><span class="hljs-number">5</span></span><span> * </span><span><span class="hljs-number">1024</span></span><span> * </span><span><span class="hljs-number">1024</span></span><span>:
        reason = </span><span><span class="hljs-string">"SIZE_LIMIT_EXCEEDED"</span></span><span>

        </span><span><span class="hljs-comment"># Move to quarantine</span></span><span>
        s3.copy_object(
            Bucket=QUARANTINE_BUCKET,
            CopySource=</span><span><span class="hljs-string">f"<span class="hljs-subst">{bucket}</span></span></span><span>/</span><span><span class="hljs-subst">{key}</span></span><span>",
            Key=key
        )
        s3.delete_object(Bucket=bucket, Key=key)

        </span><span><span class="hljs-comment"># Audit log</span></span><span>
        log_audit(
            file_name=key,
            bucket=bucket,
            decision=</span><span><span class="hljs-string">"QUARANTINED"</span></span><span>,
            reason=reason,
            confidence=</span><span><span class="hljs-string">"N/A"</span></span><span>
        )

        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"Blocked due to size limit"</span></span><span>)
        </span><span><span class="hljs-keyword">return</span></span><span> {</span><span><span class="hljs-string">"statusCode"</span></span><span>: </span><span><span class="hljs-number">200</span></span><span>}

    </span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
    </span><span><span class="hljs-comment"># STEP 5B: Rekognition moderation scan</span></span><span>
    </span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
    image_obj = s3.get_object(Bucket=bucket, Key=key)
    image_bytes = image_obj[</span><span><span class="hljs-string">"Body"</span></span><span>].read()

    response = rekognition.detect_moderation_labels(
        Image={</span><span><span class="hljs-string">"Bytes"</span></span><span>: image_bytes},
        MinConfidence=</span><span><span class="hljs-number">60</span></span><span>
    )

    </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"Rekognition response:"</span></span><span>, response)

    </span><span><span class="hljs-comment"># Find highest confidence label (if any)</span></span><span>
    max_confidence = </span><span><span class="hljs-number">0</span></span><span>
    </span><span><span class="hljs-keyword">for</span></span><span> label </span><span><span class="hljs-keyword">in</span></span><span> response[</span><span><span class="hljs-string">"ModerationLabels"</span></span><span>]:
        </span><span><span class="hljs-keyword">if</span></span><span> label[</span><span><span class="hljs-string">"Confidence"</span></span><span>] &gt; max_confidence:
            max_confidence = label[</span><span><span class="hljs-string">"Confidence"</span></span><span>]

    </span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
    </span><span><span class="hljs-comment"># STEP 5C: Decision logic</span></span><span>
    </span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> response[</span><span><span class="hljs-string">"ModerationLabels"</span></span><span>]:
        decision = </span><span><span class="hljs-string">"QUARANTINED"</span></span><span>
        reason = </span><span><span class="hljs-string">"NSFW_DETECTED"</span></span><span>
        target_bucket = QUARANTINE_BUCKET
    </span><span><span class="hljs-keyword">else</span></span><span>:
        decision = </span><span><span class="hljs-string">"ALLOWED"</span></span><span>
        reason = </span><span><span class="hljs-string">"SAFE_CONTENT"</span></span><span>
        target_bucket = ALLOWED_BUCKET

    </span><span><span class="hljs-comment"># Move image</span></span><span>
    s3.copy_object(
        Bucket=target_bucket,
        CopySource=</span><span><span class="hljs-string">f"<span class="hljs-subst">{bucket}</span></span></span><span>/</span><span><span class="hljs-subst">{key}</span></span><span>",
        Key=key
    )
    s3.delete_object(Bucket=bucket, Key=key)

    </span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
    </span><span><span class="hljs-comment"># STEP 5D: Audit logging</span></span><span>
    </span><span><span class="hljs-comment"># --------------------------------------------------</span></span><span>
    log_audit(
        file_name=key,
        bucket=bucket,
        decision=decision,
        reason=reason,
        confidence=max_confidence
    )

    </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"Decision: <span class="hljs-subst">{decision}</span></span></span><span>")
    </span><span><span class="hljs-keyword">return</span></span><span> {</span><span><span class="hljs-string">"statusCode"</span></span><span>: </span><span><span class="hljs-number">200</span></span><span>}
</span></span></code></div></div></pre>
